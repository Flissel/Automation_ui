# TRAE Remote Frontend Development Environment Variables

# API Configuration
VITE_API_URL=http://localhost:8007/api
VITE_API_BASE_URL=http://localhost:8007
VITE_WS_URL=ws://localhost:8007/ws
VITE_WS_BASE_URL=ws://localhost:8007

# Live Desktop Configuration
VITE_LIVE_DESKTOP_WS_URL=ws://localhost:8007

# Application Configuration
VITE_APP_NAME=TRAE Remote (Dev)
VITE_APP_VERSION=1.0.0-dev
VITE_APP_DESCRIPTION=High-end node system interface - Development

# Development Configuration
VITE_DEV_MODE=true
VITE_DEBUG_MODE=true
VITE_LOG_LEVEL=debug

# Feature Flags
VITE_ENABLE_WEBSOCKET=true
VITE_ENABLE_AUTOSAVE=true
VITE_ENABLE_COLLABORATION=true
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_HOT_RELOAD=true
VITE_ENABLE_DEV_TOOLS=true

# UI Configuration
VITE_DEFAULT_THEME=light
VITE_ENABLE_DARK_MODE=true
VITE_ANIMATION_DURATION=200
VITE_SHOW_GRID=true
VITE_SHOW_DEBUG_INFO=true

# Performance Configuration
VITE_MAX_NODES=1000
VITE_MAX_EDGES=2000
VITE_EXECUTION_TIMEOUT=300000
VITE_AUTOSAVE_INTERVAL=10000
VITE_WEBSOCKET_RECONNECT_INTERVAL=3000

# Security Configuration
VITE_ENABLE_CSP=false
VITE_SECURE_COOKIES=false
VITE_CORS_ENABLED=true

# Development Tools
VITE_MOCK_API=false
VITE_ENABLE_STORYBOOK=false
VITE_ENABLE_TESTING=true